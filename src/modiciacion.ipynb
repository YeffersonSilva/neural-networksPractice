    "import numpy as np\n",
    "\n",
    "entradas = np.array([-1, 7, 5])\n",
    "pesos = np.array([0.8, 0.1, 0])\n",
    "\n",
    "#e entrada p peso\n",
    "\n",
    "def soma(e, p):\n",
    "    return e.dot(p)\n",
    "\n",
    "\n",
    "s = soma(entradas, pesos)\n",
    "\n",
    "\n",
    "def stepFunction(soma):\n",
    "    if soma >= 1:\n",
    "        return 1\n",
    "    return 0\n",
    "\n",
    "\n",
    "r = stepFunction(s)"
